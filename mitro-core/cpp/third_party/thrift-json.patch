+void TSimpleJSONProtocol::pushContext(boost::shared_ptr<TJSONContext> c) {
+  contexts_.push(context_);
+  context_ = c;
+}
+
+void TSimpleJSONProtocol::popContext() {
+  context_ = contexts_.top();
+  contexts_.pop();
+}
