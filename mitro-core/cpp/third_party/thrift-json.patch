+
+void TSimpleJSONProtocol::readAllInputFromTransport(string* s) {
+  s->clear();
+
+  const uint32_t BUF_SIZE = 1024;
+  uint8_t buf[BUF_SIZE];
+  uint32_t num_read;
+
+  while ((num_read = trans_->read(buf, BUF_SIZE)) > 0) {
+    s->append(reinterpret_cast<char*>(buf), num_read); 
+  }
+}
